{
  "manifest_version": 3,
  "name": "Profile and Tabs Manager",
  "version": "1.2",
"description": "Switch profiles, manage tabs, and boost productivity with this fast and simple Chrome extension!",  "permissions": [
    "tabs",
    "windows",
    "pageCapture",
    "storage",
    "notifications"
  ],
  "host_permissions": [
    "http://localhost:3000/*"
  ],
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}