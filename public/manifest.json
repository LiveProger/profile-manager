{
  "manifest_version": 3,
  "name": "Profile and Tabs Manager",
  "version": "1.1",
"description": "A Chrome extension to manage profiles, tabs, and save pages offline as MHTML files.",  "permissions": [
    "tabs",
    "windows",
    "pageCapture",
    "storage",
    "notifications"
  ],
  "host_permissions": [
    "http://localhost:3000/*"
  ],
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}